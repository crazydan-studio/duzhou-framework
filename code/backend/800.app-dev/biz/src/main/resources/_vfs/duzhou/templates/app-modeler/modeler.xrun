<?xml version="1.0" encoding="UTF-8" ?>
<c:unit xmlns:c="c" xmlns:gen="gen" xmlns:xpl="xpl">

    <c:script><![CDATA[
        const codeGenModel = {
            'ext:moduleName': service.code,
            'ext:registerShortName': true,
            'ext:mavenGroupId': 'io.crazydan.duzhou.platform',
            'ext:mavenArtifactId': 'app-modeler',
            'ext:mavenVersion': '0.1.0-SNAPSHOT',
            'ext:dialect': 'mysql',
            entityModels: []
        };

        codeGenModel['ext:basePackageName'] =
            codeGenModel['ext:mavenGroupId'].replaceAll('-', '_') +
            '.' + codeGenModel['ext:mavenArtifactId'].replaceAll('-', '_');
        codeGenModel['ext:entityPackageName'] = codeGenModel['ext:basePackageName'] + '.orm.entity';

        $scope.setLocalValue('codeGenModel', codeGenModel);

        codeGenerator
            .withTplDir('/duzhou/templates/service')
            .execute('/', $scope);
    ]]></c:script>
</c:unit>
