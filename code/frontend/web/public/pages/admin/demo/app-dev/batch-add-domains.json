{
  "status": 0,
  "msg": "",
  "data": {
    "type": "crud",
    "syncLocation": false,
    "source": "${data}",
    "primaryField": "name",
    "checkOnItemClick": true,
    "perPage": 5,
    "keepItemSelectionOnPageChange": true,
    "headerToolbar": [
      {
        "type": "button",
        "label": "添加全部域定义",
        "level": "primary",
        "disabledOn": "${selectedItems.length > 0}",
        "onEvent": {
          "click": {
            "actions": [
              {
                "actionType": "ajax",
                "api": {
                  "url": "@mutation:DevAppDomain__batchModify",
                  "method": "post",
                  "data": { "data": "${data}" },
                  "messages": {
                    "success": "域定义添加成功！"
                  }
                }
              },
              {
                "actionType": "closeDialog",
                "expression": "${responseResult.responseStatus == 0}"
              },
              {
                "actionType": "reload",
                "componentName": "${refTableName}",
                "expression": "${responseResult.responseStatus == 0}"
              }
            ]
          }
        }
      },
      "bulkActions",
      {
        "type": "columns-toggler",
        "icon": "fas fa-cog",
        "align": "right"
      },
      {
        "type": "pagination",
        "align": "right"
      }
    ],
    "footerToolbar": ["switch-per-page", "statistics", "pagination"],
    "bulkActions": [
      {
        "label": "添加选中域定义",
        "level": "primary",
        "onEvent": {
          "click": {
            "actions": [
              {
                "actionType": "ajax",
                "api": {
                  "url": "@mutation:DevAppDomain__batchModify",
                  "method": "post",
                  "data": { "data": "${selectedItems}" },
                  "messages": {
                    "success": "域定义添加成功！"
                  }
                }
              },
              {
                "actionType": "closeDialog",
                "expression": "${responseResult.responseStatus == 0}"
              },
              {
                "actionType": "reload",
                "componentName": "${refTableName}",
                "expression": "${responseResult.responseStatus == 0}"
              }
            ]
          }
        }
      }
    ],
    "columns": [
      {
        "name": "displayName",
        "label": "名称"
      },
      {
        "name": "name",
        "label": "编码"
      },
      {
        "type": "select",
        "name": "stdDomain",
        "label": "标准域",
        "placeholder": "",
        "source": "@dict:core/std-domain",
        "static": true
      },
      {
        "type": "select",
        "name": "stdSqlType",
        "label": "数据类型",
        "placeholder": "",
        "source": "@dict:core/std-sql-type",
        "static": true
      },
      {
        "name": "precision",
        "label": "数据长度"
      },
      {
        "name": "scale",
        "label": "小数位数"
      }
    ],
    "data": {
      "data": [
        {
          "appId": "${appId}",
          "displayName": "UUID",
          "name": "uuid",
          "stdSqlType": "VARCHAR",
          "precision": 32
        },
        {
          "appId": "${appId}",
          "displayName": "数据修订版本",
          "name": "version",
          "stdSqlType": "INTEGER"
        },
        {
          "appId": "${appId}",
          "displayName": "数据软删除标记",
          "name": "delFlag",
          "stdDomain": "boolFlag",
          "stdSqlType": "TINYINT"
        },
        {
          "appId": "${appId}",
          "displayName": "布尔值",
          "name": "boolFlag",
          "stdSqlType": "TINYINT"
        },
        {
          "appId": "${appId}",
          "displayName": "数据创建人",
          "name": "createdBy",
          "stdSqlType": "VARCHAR",
          "precision": 50
        },
        {
          "appId": "${appId}",
          "displayName": "数据更新人",
          "name": "updatedBy",
          "stdSqlType": "VARCHAR",
          "precision": 50
        },
        {
          "appId": "${appId}",
          "displayName": "数据创建时间",
          "name": "createTime",
          "stdSqlType": "TIMESTAMP"
        },
        {
          "appId": "${appId}",
          "displayName": "数据更新时间",
          "name": "updateTime",
          "stdSqlType": "TIMESTAMP"
        },
        {
          "appId": "${appId}",
          "displayName": "图片",
          "name": "image",
          "stdDomain": "file",
          "stdSqlType": "VARCHAR",
          "precision": 200
        },
        {
          "appId": "${appId}",
          "displayName": "附件",
          "name": "file",
          "stdDomain": "file",
          "stdSqlType": "VARCHAR",
          "precision": 200
        },
        {
          "appId": "${appId}",
          "displayName": "邮箱",
          "name": "email",
          "stdSqlType": "VARCHAR",
          "precision": 100
        },
        {
          "appId": "${appId}",
          "displayName": "手机号",
          "name": "phone",
          "stdSqlType": "VARCHAR",
          "precision": 100
        },
        {
          "appId": "${appId}",
          "displayName": "JSON（1KB）",
          "name": "json-1k",
          "stdDomain": "json",
          "stdSqlType": "VARCHAR",
          "precision": 1024
        },
        {
          "appId": "${appId}",
          "displayName": "JSON（4KB）",
          "name": "json-4k",
          "stdDomain": "json",
          "stdSqlType": "VARCHAR",
          "precision": 4096
        },
        {
          "appId": "${appId}",
          "displayName": "JSON（16MB）",
          "name": "json-16m",
          "stdDomain": "json",
          "stdSqlType": "VARCHAR",
          "precision": 16777216
        }
      ]
    }
  }
}
