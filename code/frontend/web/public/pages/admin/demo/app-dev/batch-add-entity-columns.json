{
  "status": 0,
  "msg": "",
  "data": {
    "type": "crud",
    "syncLocation": false,
    "source": "${data}",
    "primaryField": "name",
    "checkOnItemClick": true,
    "perPage": 5,
    "keepItemSelectionOnPageChange": true,
    "headerToolbar": [
      {
        "type": "button",
        "label": "添加全部字段",
        "level": "primary",
        "disabledOn": "${selectedItems.length > 0}",
        "onEvent": {
          "click": {
            "actions": [
              {
                "actionType": "ajax",
                "api": {
                  "url": "@mutation:DevAppEntityColumn__batchModify",
                  "method": "post",
                  "data": { "data": "${data}" },
                  "requestAdaptor": "return {...api, data: {...api.data, data: [...api.data.data.map((d, i) => { d = {...d, propId: i + 1}; return d; })]}};",
                  "messages": {
                    "success": "字段添加成功！"
                  }
                }
              },
              {
                "actionType": "closeDialog",
                "expression": "${responseResult.responseStatus == 0}"
              },
              {
                "actionType": "reload",
                "componentName": "${refTableName}",
                "expression": "${responseResult.responseStatus == 0}"
              }
            ]
          }
        }
      },
      "bulkActions",
      {
        "type": "columns-toggler",
        "icon": "fas fa-cog",
        "align": "right"
      },
      {
        "type": "pagination",
        "align": "right"
      }
    ],
    "footerToolbar": ["switch-per-page", "statistics", "pagination"],
    "bulkActions": [
      {
        "label": "添加选中字段",
        "level": "primary",
        "onEvent": {
          "click": {
            "actions": [
              {
                "actionType": "ajax",
                "api": {
                  "url": "@mutation:DevAppEntityColumn__batchModify",
                  "method": "post",
                  "data": { "data": "${selectedItems}" },
                  "requestAdaptor": "return {...api, data: {...api.data, data: [...api.data.data.map((d, i) => { d = {...d, propId: i + 1}; return d; })]}};",
                  "messages": {
                    "success": "字段添加成功！"
                  }
                }
              },
              {
                "actionType": "closeDialog",
                "expression": "${responseResult.responseStatus == 0}"
              },
              {
                "actionType": "reload",
                "componentName": "${refTableName}",
                "expression": "${responseResult.responseStatus == 0}"
              }
            ]
          }
        }
      }
    ],
    "columns": [
      {
        "name": "displayName",
        "label": "名称"
      },
      {
        "name": "name",
        "label": "字段名"
      },
      {
        "type": "switch",
        "name": "primary",
        "label": "是否主键",
        "static": true,
        "onText": "是",
        "offText": "否"
      },
      {
        "type": "switch",
        "name": "mandatory",
        "label": "是否非空",
        "static": true,
        "onText": "是",
        "offText": "否"
      },
      {
        "type": "list-select",
        "name": "tagSet",
        "label": "标签",
        "static": true,
        "multiple": true,
        "placeholder": "",
        "source": "@dict:app_dev/entity-column-tagset"
      },
      {
        "type": "select",
        "name": "stdSqlType",
        "label": "字段类型",
        "static": true,
        "placeholder": "",
        "source": "@dict:core/std-sql-type"
      },
      {
        "name": "defaultValue",
        "label": "默认值"
      },
      {
        "name": "precision",
        "label": "字段长度"
      },
      {
        "name": "scale",
        "label": "小数精度"
      },
      {
        "type": "select",
        "name": "domainName",
        "label": "数据域",
        "static": true,
        "placeholder": "",
        "source": "@query:DevAppDomain__findList/label:displayName,value:name?filter_appId__eq=${appId}"
      },
      {
        "type": "select",
        "name": "dictName",
        "label": "字典",
        "static": true,
        "placeholder": "",
        "source": "@query:DevAppDict__findList/label:displayName,value:name?filter_appId__eq=${appId}"
      }
    ],
    "data": {
      "data": [
        {
          "entityId": "${entityId}",
          "displayName": "ID",
          "name": "ID",
          "primary": true,
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "tagSet": "seq",
          "domainName": "uuid",
          "precision": 32
        },
        {
          "entityId": "${entityId}",
          "displayName": "显示名称",
          "name": "DISPLAY_NAME",
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "precision": 100,
          "tagSet": "disp"
        },
        {
          "entityId": "${entityId}",
          "displayName": "编码",
          "name": "CODE",
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "precision": 50
        },
        {
          "entityId": "${entityId}",
          "displayName": "数据修订版本",
          "name": "VER",
          "mandatory": true,
          "stdSqlType": "INTEGER",
          "defaultValue": 0,
          "domainName": "version"
        },
        {
          "entityId": "${entityId}",
          "displayName": "数据软删除标记",
          "name": "DEL",
          "mandatory": true,
          "stdSqlType": "TINYINT",
          "defaultValue": 0,
          "domainName": "delFlag"
        },
        {
          "entityId": "${entityId}",
          "displayName": "创建人",
          "name": "CREATED_BY",
          "stdSqlType": "VARCHAR",
          "domainName": "createdBy"
        },
        {
          "entityId": "${entityId}",
          "displayName": "更新人",
          "name": "UPDATED_BY",
          "stdSqlType": "VARCHAR",
          "domainName": "updatedBy"
        },
        {
          "entityId": "${entityId}",
          "displayName": "创建时间",
          "name": "CREATED_TIME",
          "mandatory": true,
          "stdSqlType": "DATETIME",
          "domainName": "createTime"
        },
        {
          "entityId": "${entityId}",
          "displayName": "更新时间",
          "name": "UPDATED_TIME",
          "mandatory": true,
          "stdSqlType": "DATETIME",
          "domainName": "updateTime"
        }
      ]
    }
  }
}
