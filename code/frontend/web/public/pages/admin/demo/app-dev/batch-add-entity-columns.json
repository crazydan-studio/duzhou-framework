{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "actions": [],
    "mode": "horizontal",
    "wrapWithPanel": false,
    "onEvent": {
      "submitSucc": {
        "actions": [
          {
            "actionType": "closeDialog"
          },
          {
            "actionType": "reload"
          }
        ]
      }
    },
    "api": {
      "url": "@mutation:DevAppEntityColumn__batchModify",
      "requestAdaptor": "return {...api, data: {...api.data, data: [...api.data.data.map((d, i) => { d = {...d, propId: i + 1}; return d; })]}};"
    },
    "data": {
      "data": [
        {
          "entityId": "${entityId}",
          "displayName": "ID",
          "name": "ID",
          "primary": true,
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "tagSet": "seq",
          "domainName": "uuid",
          "precision": 32
        },
        {
          "entityId": "${entityId}",
          "displayName": "显示名称",
          "name": "DISPLAY_NAME",
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "precision": 100,
          "tagSet": "disp"
        },
        {
          "entityId": "${entityId}",
          "displayName": "编码",
          "name": "CODE",
          "mandatory": true,
          "stdSqlType": "VARCHAR",
          "precision": 50
        },
        {
          "entityId": "${entityId}",
          "displayName": "数据修订版本",
          "name": "VER",
          "mandatory": true,
          "stdSqlType": "INTEGER",
          "defaultValue": 0,
          "domainName": "version"
        },
        {
          "entityId": "${entityId}",
          "displayName": "数据软删除标记",
          "name": "DEL",
          "mandatory": true,
          "stdSqlType": "TINYINT",
          "defaultValue": 0,
          "domainName": "delFlag"
        },
        {
          "entityId": "${entityId}",
          "displayName": "创建人",
          "name": "CREATED_BY",
          "stdSqlType": "VARCHAR",
          "domainName": "createdBy"
        },
        {
          "entityId": "${entityId}",
          "displayName": "更新人",
          "name": "UPDATED_BY",
          "stdSqlType": "VARCHAR",
          "domainName": "updatedBy"
        },
        {
          "entityId": "${entityId}",
          "displayName": "创建时间",
          "name": "CREATED_TIME",
          "mandatory": true,
          "stdSqlType": "DATETIME",
          "domainName": "createTime"
        },
        {
          "entityId": "${entityId}",
          "displayName": "更新时间",
          "name": "UPDATED_TIME",
          "mandatory": true,
          "stdSqlType": "DATETIME",
          "domainName": "updateTime"
        }
      ]
    },
    "body": [
      {
        "type": "input-table",
        "name": "data",
        "label": false,
        "needConfirm": true,
        "removable": true,
        "perPage": 5,
        "columns": [
          {
            "name": "displayName",
            "label": "名称"
          },
          {
            "name": "name",
            "label": "字段名"
          },
          {
            "type": "switch",
            "name": "primary",
            "label": "是否主键",
            "static": true,
            "onText": "是",
            "offText": "否"
          },
          {
            "type": "switch",
            "name": "mandatory",
            "label": "是否非空",
            "static": true,
            "onText": "是",
            "offText": "否"
          },
          {
            "type": "list-select",
            "name": "tagSet",
            "label": "标签",
            "static": true,
            "multiple": true,
            "placeholder": "",
            "source": "@dict:app_dev/entity-column-tagset"
          },
          {
            "type": "select",
            "name": "stdSqlType",
            "label": "字段类型",
            "static": true,
            "placeholder": "",
            "source": "@dict:core/std-sql-type"
          },
          {
            "name": "defaultValue",
            "label": "默认值"
          },
          {
            "name": "precision",
            "label": "字段长度"
          },
          {
            "name": "scale",
            "label": "小数精度"
          },
          {
            "type": "select",
            "name": "domainName",
            "label": "数据域",
            "static": true,
            "placeholder": "",
            "source": "@query:DevAppDomain__findList/label:displayName,value:name?appId=${appId}"
          },
          {
            "type": "select",
            "name": "dictName",
            "label": "字典",
            "static": true,
            "placeholder": "",
            "source": "@query:DevAppDict__findList/label:displayName,value:name?appId=${appId}"
          }
        ]
      }
    ]
  }
}
