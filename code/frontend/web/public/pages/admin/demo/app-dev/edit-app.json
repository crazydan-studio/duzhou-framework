{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "actions": [],
    "mode": "horizontal",
    "wrapWithPanel": false,
    "autoFocus": true,
    "onEvent": {
      "submitSucc": {
        "actions": [
          {
            "actionType": "closeDialog"
          },
          {
            "actionType": "reload",
            "componentName": "${refTableName}"
          }
        ]
      }
    },
    "initApi": {
      "url": "@query:DevApp__get?id=${appId}",
      "gql:selection": "id,logo,displayName,code1,code2,version,classPkgName,description,license,copyright"
    },
    "api": {
      "url": "@mutation:DevApp__update?id=${appId}",
      "gql:selection": "id"
    },
    "body": [
      {
        "type": "input-image",
        "name": "logo",
        "label": "图标",
        "required": false,
        "accept": ".jpeg,.jpg,.png,.gif,.svg",
        "uploadBtnText": "点击上传应用图标",
        "receiver": "/f/upload?bizObjName=DevApp&fieldName=logo&bizObjId=${appId}"
      },
      {
        "type": "input-text",
        "name": "displayName",
        "label": "名称",
        "required": true
      },
      {
        "type": "input-text",
        "name": "code",
        "label": "编码",
        "required": true,
        "disabled": true,
        "value": "${code1}-${code2}"
      },
      {
        "type": "input-text",
        "name": "version",
        "label": "版本号",
        "required": true
      },
      {
        "type": "input-text",
        "name": "classPkgName",
        "label": "代码包名",
        "required": true
      },
      {
        "type": "editor",
        "name": "description",
        "label": "说明",
        "language": "markdown"
      },
      {
        "type": "editor",
        "name": "license",
        "label": "许可协议",
        "language": "markdown"
      },
      {
        "type": "editor",
        "name": "copyright",
        "label": "版权声明",
        "language": "markdown"
      }
    ]
  }
}
