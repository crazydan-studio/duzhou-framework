{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "mode": "horizontal",
    "wrapWithPanel": true,
    "initApi": {
      "url": "@query:DevAppEntity__get?id=${entityId}",
      "gql:selection": "uniqueKeys",
      "adaptor": "return {...payload, data:{...payload.data, uniqueKeys: JSON.parse(payload.data.uniqueKeys)}}"
    },
    "api": {
      "url": "@mutation:DevAppEntity__update?id=${entityId}",
      "gql:selection": "id",
      "requestAdaptor": "return {...api, data: {...api.data, uniqueKeys: JSON.stringify(api.data.uniqueKeys)}};"
    },
    "body": [
      {
        "type": "combo",
        "name": "uniqueKeys",
        "label": "唯一键列表",
        "value": "${uniqueKeys}",
        "multiple": true,
        "flat": true,
        "items": [
          {
            "type": "select",
            "label": false,
            "multiple": true,
            "mode": "horizontal",
            "source": {
              "url": "@query:DevAppEntityColumn__findList/label:name,value:name",
              "data": {
                "filter_entityId__eq": "${entityId}"
              }
            }
          }
        ]
      }
    ]
  }
}
