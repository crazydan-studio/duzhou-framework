{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "actions": [],
    "mode": "horizontal",
    "wrapWithPanel": false,
    "autoFocus": true,
    "onEvent": {
      "submitSucc": {
        "actions": [
          {
            "actionType": "closeDialog"
          },
          {
            "actionType": "reload",
            "componentName": "${refTableName}"
          }
        ]
      }
    },
    "api": {
      "url": "@mutation:DevAppDict__save",
      "gql:selection": "id"
    },
    "body": [
      {
        "type": "group",
        "body": [
          {
            "type": "input-text",
            "name": "displayName",
            "label": "名称",
            "fixed": "left",
            "required": true
          },
          {
            "type": "input-text",
            "name": "name",
            "label": "编码",
            "required": true
          }
        ]
      },
      {
        "type": "group",
        "body": [
          {
            "type": "service",
            "className": "amis-Form-item",
            "schemaApi": "get:/pages/admin/demo/app-dev/pick-app.json"
          },
          {
            "type": "select",
            "name": "optionType",
            "label": "字典项类型",
            "source": "@dict:core/std-data-type",
            "required": true
          }
        ]
      },
      {
        "type": "input-table",
        "name": "options",
        "label": "字典项列表",
        "required": true,
        "needConfirm": false,
        "addable": true,
        "removable": true,
        "columns": [
          {
            "type": "input-text",
            "name": "code",
            "label": "编码",
            "required": true
          },
          {
            "type": "input-text",
            "name": "value",
            "label": "值",
            "required": true
          },
          {
            "type": "input-text",
            "name": "label",
            "label": "名称"
          },
          {
            "type": "input-text",
            "name": "description",
            "label": "描述"
          }
        ]
      }
    ]
  }
}
