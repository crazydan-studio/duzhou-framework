{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "actions": [],
    "mode": "horizontal",
    "wrapWithPanel": false,
    "autoFocus": true,
    "onEvent": {
      "submitSucc": {
        "actions": [
          {
            "actionType": "closeDialog"
          },
          {
            "actionType": "reload"
          }
        ]
      }
    },
    "api": {
      "url": "@mutation:DevApp__save",
      "gql:selection": "id",
      "requestAdaptor": "return {...api, data: {...api.data, code: api.data.code1 + '-' + api.data.code2, code1: undefined, code2: undefined}};"
    },
    "body": [
      {
        "type": "input-image",
        "name": "logo",
        "label": "图标",
        "required": true,
        "accept": ".jpeg,.jpg,.png,.gif,.svg",
        "uploadBtnText": "点击上传应用图标",
        "receiver": "/f/upload?bizObjName=DevApp&fieldName=logo"
      },
      {
        "type": "input-text",
        "name": "displayName",
        "label": "名称",
        "required": true
      },
      {
        "type": "input-group",
        "name": "code",
        "label": "编码",
        "required": true,
        "body": [
          {
            "type": "input-text",
            "name": "code1",
            "required": true,
            "label": "第一级编码",
            "placeholder": "第一级编码"
          },
          {
            "type": "input-text",
            "name": "code2",
            "prefix": "-",
            "required": true,
            "label": "第二级编码",
            "placeholder": "第二级编码"
          }
        ]
      },
      {
        "type": "input-text",
        "name": "version",
        "label": "版本号",
        "required": true,
        "value": "0.1.0-SNAPSHOT"
      },
      {
        "type": "input-text",
        "name": "classPkgName",
        "label": "代码包名",
        "required": true
      }
    ]
  }
}
