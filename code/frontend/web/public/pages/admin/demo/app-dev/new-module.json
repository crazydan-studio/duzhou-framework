{
  "status": 0,
  "msg": "",
  "data": {
    "type": "form",
    "title": "",
    "actions": [],
    "mode": "horizontal",
    "wrapWithPanel": false,
    "autoFocus": true,
    "onEvent": {
      "submitSucc": {
        "actions": [
          {
            "actionType": "closeDialog"
          },
          {
            "actionType": "reload",
            "componentName": "${refTableName}"
          }
        ]
      }
    },
    "api": {
      "url": "@mutation:DevAppModule__save",
      "gql:selection": "id"
    },
    "body": [
      {
        "type": "input-text",
        "name": "displayName",
        "label": "名称",
        "required": true
      },
      {
        "type": "input-text",
        "name": "code",
        "label": "编码",
        "required": true
      },
      {
        "type": "service",
        "className": "amis-Form-item",
        "schemaApi": "get:/pages/admin/demo/app-dev/pick-app.json"
      },
      {
        "type": "select",
        "name": "webStarterType",
        "label": "Web 启动器类型",
        "source": "@dict:app_modeler/web-starter-type",
        "value": "spring",
        "required": true
      },
      {
        "type": "select",
        "name": "dbDialect",
        "label": "支持的数据库类型",
        "multiple": true,
        "clearable": true,
        "source": "@dict:app_modeler/db-dialect"
      },
      {
        "type": "input-text",
        "name": "description",
        "label": "说明"
      }
    ]
  }
}
